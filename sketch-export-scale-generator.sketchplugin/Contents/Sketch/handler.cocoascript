
@import 'export.cocoascript'

var exportIOS = function(context) {
  setScale([1,2,3]);
	runExport(context);
}

var exportAndroid = function(context) {

  /*
  mdpi = ~160dpi = 1×, or 100%
  hdpi = ~240dpi = 1.5×, or 150%
  xhdpi = ~320dpi = 2×, or 200%
  xxhdpi = ~480dpi = 3×, or 300%
  xxxhdpi = ~640dpi = 4×, or 400%
  */
  setScale([1,1.5,2,3]);
	runExport(context);
}
